FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     laplacianFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.1;

deltaT          0.005;

writeControl    runTime;

writeInterval   0.1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
  probes1
  {
  type probes;

  functionObjectLibs ("libsampling.so");

  region region0;

  probeLocations
  (
  (0.25 0.30 0.05)
  (0.5 0.25 0.05)
  (0.75 0.11 0.05)
  );

  fields
  (
  T
  );

  outputControl timeStep;
  outputInterval 1;
  }
}
